    ctx.fill();
}

// Funci=C3=B3n para calcular el producto cruzado
function productoCruz(p1, p2, p3) {
    const x1 =3D p2.getX() - p1.getX();
    const y1 =3D p2.getY() - p1.getY();
    const x2 =3D p3.getX() - p2.getX();
    const y2 =3D p3.getY() - p2.getY();
    return (x1 * y2) - (y1 * x2);
}

// Funci=C3=B3n para determinar si el pol=C3=ADgono es convexo
function esConvexo(puntos) {
    if (puntos.length &lt; 4) return true; // Tri=C3=A1ngulo siempre convex=
o

    let signo =3D 0;
    const n =3D puntos.length;

    for (let i =3D 0; i &lt; n; i++) {
        const p0 =3D puntos[i];
        const p1 =3D puntos[(i + 1) % n];
        const p2 =3D puntos[(i + 2) % n];
        const cross =3D productoCruz(p0, p1, p2);

        if (cross !=3D=3D 0) {
            if (signo =3D=3D=3D 0) {
                signo =3D cross &gt; 0 ? 1 : -1;
            } else {
                if ((cross &gt; 0 &amp;&amp; signo =3D=3D=3D -1) || (cross =
&lt; 0 &amp;&amp; signo =3D=3D=3D 1)) {
                    return false; // Es c=C3=B3ncavo
                }
            }
        }
    }
    return true; // Es convexo
}

// Funci=C3=B3n para calcular la orientaci=C3=B3n de tres puntos
function orientacion(a, b, c) {
    const val =3D (b.getX() - a.getX()) * (c.getY() - a.getY()) -=20
                (b.getY() - a.getY()) * (c.getX() - a.getX());
    if (val =3D=3D=3D 0) return 0; // Colinear
    return (val &gt; 0) ? 1 : 2; // 1: Horario, 2: Anti-horario
}

// Funci=C3=B3n para verificar si dos segmentos de l=C3=ADnea se cruzan
function segmentosCruzan(p1, p2, p3, p4) {
