// Funci=C3=B3n para calcular el centroide de los puntos
function calcularCentroide(puntos) {
    let sumaX =3D 0;
    let sumaY =3D 0;
    const n =3D puntos.length;

    for (let i =3D 0; i &lt; n; i++) {
        sumaX +=3D puntos[i].getX();
        sumaY +=3D puntos[i].getY();
    }

    return {
        x: sumaX / n,
        y: sumaY / n
    };
}

// Funci=C3=B3n para ordenar los puntos por =C3=A1ngulo desde el centroide
function ordenarPuntosPorAngulo(puntos) {
    const centro =3D calcularCentroide(puntos);

    // Calcular el =C3=A1ngulo de cada punto respecto al centro
    puntos.sort((a, b) =3D&gt; {
