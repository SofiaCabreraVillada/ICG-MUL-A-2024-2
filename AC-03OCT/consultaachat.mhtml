        for (let j =3D i + 1; j &lt; n; j++) {
            // Evitar l=C3=ADneas adyacentes y la misma l=C3=ADnea
            if (Math.abs(i - j) &lt;=3D 1 || (i =3D=3D=3D 0 &amp;&amp; j =
=3D=3D=3D n - 1)) {
