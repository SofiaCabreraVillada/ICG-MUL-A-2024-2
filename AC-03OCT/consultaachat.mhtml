// Funci=C3=B3n para convertir HEX a RGBA
function hexToRgba(hex, alpha) {
    const r =3D parseInt(hex.slice(1, 3), 16);
    const g =3D parseInt(hex.slice(3, 5), 16);
    const b =3D parseInt(hex.slice(5, 7), 16);
    return <code>rgba(${r}, ${g}, ${b}, ${alpha})</code>;
}

// Funci=C3=B3n para dibujar el pol=C3=ADgono
function dibujarPoligono(ctx, puntos) {
    if (puntos.length =3D=3D=3D 0) return;

    ctx.beginPath();
    ctx.moveTo(puntos[0].getX(), puntos[0].getY());

    for (let i =3D 1; i &lt; puntos.length; i++) {
        ctx.lineTo(puntos[i].getX(), puntos[i].getY());
    }
    ctx.closePath();

    // Obtener los colores seleccionados por el usuario
    const colorContorno =3D document.getElementById('colorContorno').value;
    const colorRelleno =3D document.getElementById('colorRelleno').value;

    // Establecer el color y grosor del contorno
    ctx.strokeStyle =3D colorContorno; // **Color del contorno**
    ctx.lineWidth =3D 2;               // **Grosor del contorno**
    ctx.stroke();

    // Rellenar el pol=C3=ADgono con el color seleccionado y opacidad
    ctx.fillStyle =3D hexToRgba(colorRelleno, 0.1); // **Color de relleno c=
on opacidad**
    ctx.fill();
}

// Funci=C3=B3n para dibujar las conexiones al centroide
function dibujarConexionesAlCentroide(ctx, puntos, centro) {
    if (puntos.length =3D=3D=3D 0 || !centro) return;

    // Obtener los colores seleccionados por el usuario
    const colorConexiones =3D document.getElementById('colorConexiones').va=
lue;
